<resources>

    <!-- Base application theme. -->
    <style name="AppBaseTheme" parent="Theme.AppCompat.Light.DarkActionBar">


    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <item name="colorPrimary">@color/color_h</item>
        <item name="colorPrimaryDark">@color/color_i</item>
        <item name="colorAccent">@color/color_h</item>
    </style>


    <style name="AppCompatTheme" parent="Theme.AppCompat.Light.DarkActionBar">
       <item name="windowActionBar">false</item>
    </style>
    <!-- Application theme. -->
    <style name="AppThemeNoAction" parent="AppCompatTheme">
        <item name="colorPrimary">@color/color_h</item>
        <item name="colorPrimaryDark">@color/color_i</item>
        <item name="colorAccent">@color/color_h</item>
    </style>



</resources>
